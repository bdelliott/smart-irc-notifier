# Place this in .irssi/server-irc-notifier.cfg
[general]
idle = 60
poll_interval = 1.0
notifier = email

[email]
smtp_host = <YOUR-SMTP-HOST-HERE>
smtp_password = <YOUR-PASSWORD-HERE>
smtp_user = <YOUR-USERNAME-HERE>
from_email = <YOUR-EMAIL-HERE>
to_email = <YOUR-SMS-EMAIL-GATEWAY-ADDRESS-HERE>
subject = <YOUR-SUBJECT-OVERRIDE-HERE>

# Set to 1 to output SMTP debugging info
debuglevel=0
